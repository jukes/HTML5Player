{
    "manifest_version": 2,
    "name": "HTML5Player",
    "description": "Play audio files",
    "version": "1.0",
    "permissions": [
        "https://secure.flickr.com/",
        "http://www.last.fm/api"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    }
}